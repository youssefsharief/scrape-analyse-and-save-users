{
  "name": "ask-jsdom",
  "version": "1.0.0",
  "description": "Scraper that uses JSDom. Tested by Mocha",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "mocha test/unit-tests/*.ts",
    "test:all": "mocha",
    "forever": "forever start -a -l NUL dist/main.js",
    "list": "forever list",
    "s": "forever stop 0",
    "log": "forever logs"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^6.2.0",
    "expect": "^24.1.0",
    "forever": "^0.15.3",
    "inversify": "^5.0.1",
    "jsdom": "^13.2.0",
    "mongoose": "^5.4.11",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "rimraf": "^2.6.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/expect": "^1.20.4",
    "@types/jsdom": "^12.2.2",
    "@types/mocha": "^5.2.6",
    "@types/mongoose": "^5.3.17",
    "@types/node": "^10.12.21",
    "@types/request": "^2.48.1",
    "mocha": "^10.2.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  },
  "main": ".prettierrc.js",
  "directories": {
    "test": "test"
  }
}
